#: ../panel-plugin/verve.c:98
#, c-format
msgid "Could not execute command (exit status %d)"
msgstr "Не може да се изпълни командата (състояние на изхода %d)"

#. Generate error message
#: ../panel-plugin/verve-plugin.c:409
msgid "Could not execute command:"
msgstr "Не може да се изпълни команда:"

#. Create properties dialog
#: ../panel-plugin/verve-plugin.c:1031
msgid "Verve"
msgstr "Verve"

#: ../panel-plugin/verve-plugin.c:1054
msgid "General"
msgstr "Общи"

#. Frame for appearance settings
#: ../panel-plugin/verve-plugin.c:1059
msgid "Appearance"
msgstr "Външен вид"

#. Plugin size label
#: ../panel-plugin/verve-plugin.c:1075
msgid "Width (in chars):"
msgstr "Ширина (в символи):"

#. Plugin label label
#: ../panel-plugin/verve-plugin.c:1100
msgid "Label:"
msgstr "Етикет:"

#. Frame for behaviour settings
#: ../panel-plugin/verve-plugin.c:1117
msgid "History"
msgstr "История"

#. History length label
#: ../panel-plugin/verve-plugin.c:1133
msgid "Number of saved history items:"
msgstr "Брой на записаните елементи от историята:"

